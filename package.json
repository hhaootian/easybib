{
	"name": "easybib",
	"displayName": "easybib",
	"description": "make LaTeX bib easier",
	"version": "0.1.0",
	"publisher": "htian97",
	"icon": "icons/icon.png",
	"license": "GPLv3",
	"repository": {
		"url": "https://github.com/htian97/easybib",
		"type": "git"
	},
	"homepage": "https://github.com/htian97/easybib",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:easybib.lookup"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "easybib.lookup",
				"title": "lookup",
				"category": "easybib"
			}
		],
		"keybindings": [
			{
				"command": "easybib.lookup",
				"key": "ctrl+alt+l",
				"mac": "shift+cmd+l"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.63.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"axios": "^0.24.0"
	}
}
